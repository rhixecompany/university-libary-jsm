{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.minimap.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 17,
    "editor.glyphMargin": false,
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "none",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "workbench.startupEditor": "none",
    "notebook.output.scrolling": true,
    "files.associations": {
        "**/.css": "tailwindcss",
        // "**/.scss": "tailwindcss"
    },
    "files.exclude": {
        "**/.csv": true,
        "**/.parquet": true,
        "**/.pkl": true,
        "**/.xls": true,
        "out": false // set this to true to hide the "out" folder with the compiled JS files
    },
    "search.exclude": {
        "out": true // set this to false to include "out" folder in search results
    },
    "workbench.colorTheme": "React Dev Theme (dark)",
    "breadcrumbs.enabled": false,
    "explorer.decorations.badges": false,
    "explorer.decorations.colors": false,
    // "files.autoSave": "afterDelay",
    "git.enableSmartCommit": true,
    "window.commandCenter": true,
    "markdown.validate.enabled": true,
    "markdown.updateLinksOnFileMove.enabled": "prompt",
    // Turn off tsc task auto detection since we have the necessary tasks as npm scripts
    "typescript.tsc.autoDetect": "on",
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.integrated.ignoreBracketedPasteMode": true,
    "terminal.integrated.minimumContrastRatio": 1,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnType": true,
        "editor.formatOnSave": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.formatOnSave": true
    },
    "[dotenv]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "WillLuke.nextjs.addTypesOnSave": true,
    "WillLuke.nextjs.hasPrompted": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[tailwindcss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[properties]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "sqltools.connections": [
        {
            "pgOptions": {
                "ssl": {
                    "rejectUnauthorized": true
                }
            },
            "previewLimit": 50,
            "server": "localhost",
            "driver": "PostgreSQL",
            "name": "neon",
            "group": "neon",
            "database": "npg_yeYK3aZx8LWw@ep-misty-truth-a85d73oi-pooler.eastus2.azure.neon.tech",
            "username": "neondb_owner",
            "connectString": "postgresql://neondb_owner:npg_yeYK3aZx8LWw@ep-misty-truth-a85d73oi-pooler.eastus2.azure.neon.tech/neondb?sslmode=require&channel_binding=require"
        }
    ],
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    }
}